#include<bits/stdc++.h>
using namespace std;
int isSubset(int arr[],int size,int sum )
{
    int t[size+1][sum+1];
    //initialise
    
    for(int i=0;i<size+1;i++)
        for(int j=0;j<sum+1;j++)
            {
                if(i==0) t[i][j]=0;
                if(j==0) t[i][j]=1;
            }

    //converting choice diagram to iterative
    for(int i=1;i<size+1;i++)
        for(int j=1;j<sum+1;j++)
            if(arr[i-1]<=j)
                t[i][j]=(t[i-1][j] + t[i-1][j-arr[i-1]])%1000000007;
            else
                t[i][j]=t[i-1][j]%1000000007;
    return t[size][sum];            
}
int main() {
	//code
	int test;
	cin>>test;
	while(test--)
	{
	    int size;
	    cin>>size;
	    int arr[size];
	    int sum;
	    for(int i=0;i<size;i++)
	        cin>>arr[i];
        cin>>sum;
        cout<<isSubset(arr,size,sum)<<endl;
	    	}
    return 0;
}
