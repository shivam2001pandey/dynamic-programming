#include <bits/stdc++.h>
using namespace std;

int knapsack(int wt[],int val[],int n,int w)
{
    int t[n+1][w+1];
    //initialisation
    for(int i=0;i<n+1;i++)
        for(int j=0;j<w+1;j++)
            if(i==0 || j==0)
                t[i][j]=0;
                
    //iterative of choice diagram
    for(int i=1;i<n+1;i++)
        for(int j=1;j<w+1;j++)
        {
            if(wt[i-1]<=j)
                t[i][j]=max(val[i-1]+t[i-1][j-wt[i-1]],
                            t[i-1][j]);
            else
                t[i][j]=t[i-1][j];
        }
    return t[n][w];    
}
int main() {
	//code
	int test;
	cin>>test;
	while(test--)
	{
	    int n,w;
	    cin>>n;
	    cin>>w;
	    int val[n];
	    int wt[n];
	    for(int i=0;i<n;i++)    cin>>val[i];
	    for(int i=0;i<n;i++)    cin>>wt[i];
	    cout<<knapsack(wt,val,n,w)<<endl;
	    
	}
	return 0;
}
